{
  "type": "module",
  "name": "@evilkiwi/devon",
  "version": "2.3.1",
  "description": "Docker-based Monorepo Local Development CLI",
  "files": [
    "bin",
    "build"
  ],
  "exports": {
    ".": "./build/index.mjs",
    "./cli": "./bin/devon.mjs"
  },
  "bin": "./bin/devon.mjs",
  "main": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "license": "GPL-3.0-only",
  "author": {
    "name": "Evil Kiwi Limited",
    "url": "https://evil.kiwi",
    "email": "support@evil.kiwi"
  },
  "homepage": "https://github.com/evilkiwi/devon",
  "bugs": {
    "url": "https://github.com/evilkiwi/devon/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evilkiwi/devon.git"
  },
  "keywords": [
    "docker",
    "dev",
    "environment",
    "development",
    "ssl",
    "cli",
    "local",
    "monorepo"
  ],
  "scripts": {
    "clean": "rimraf build",
    "prepack": "pnpm run build",
    "build": "pnpm run clean && pnpm run compile",
    "compile": "NODE_ENV=production unbuild",
    "fetch": "wget -O - https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json | json2ts --style.singleQuote > ./src/types/generated.ts"
  },
  "dependencies": {
    "@oclif/core": "^1.21.0",
    "colorette": "^2.0.19",
    "commander": "^9.4.1",
    "conf": "^10.2.0",
    "consola": "^2.15.3",
    "docker-compose": "^0.23.17",
    "fs-extra": "^11.1.0",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "inquirer": "^9.1.4",
    "is-wsl": "^2.2.0",
    "js-yaml": "^4.1.0",
    "ora": "^6.1.2",
    "sudo-prompt": "^9.2.1",
    "tsx": "^3.12.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^9.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.13",
    "json-schema-to-typescript": "^11.0.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2"
  },
  "engines": {
    "node": "^16.0.0 || ^17.0.0 || ^18.0.0"
  }
}
